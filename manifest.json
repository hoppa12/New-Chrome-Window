

{
  "manifest_version": 2,

  "name": "Resized Window",
  "description": "Create a new custom sized window",
  "version": "0.1",
  
  
  "permissions": [
    "activeTab"
  ],

  
  "browser_action": {
    "default_title": "NewSize Window",
    "default_popup": "options.html",
    "default_icon": {
      "19": "img/sub19.png",
      "38": "img/sub38.png",
      "128": "img/sub128.png"
    }
  }
}
